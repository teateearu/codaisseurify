<%= stylesheet_link_tag "styles.css" %>

<div id:"songForm>"<%= form_for [@artist, @artist.songs.build] do |f| %>

  <p class="whitetext"><%= f.label :title %></p>
  <p class="blacktext"><%= f.text_field :title, id:"addTitle" %></p>
  <p class="whitetext"><%= f.label :year %></p>
  <p class="blacktext"><%= f.number_field :year, id:"addYear" %></p>

  <%= f.submit %>

<% end %>
</div>
